
<h1>New Pictures!</h1>

<%= form_with scope: :picture, url: pictures_path, local: true do |form| %>

  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p>

  <p>
    <%= form.label :text %><br>
    <%= form.text_area :text %>
  </p>

  <p>
    <%= form.submit %>
  </p>


  <%= form_with scope: :picture, url: pictures_path, local: true do |form| %>
  
<% end %>

<%= link_to 'Back', pictures_path %>

<% end %>
