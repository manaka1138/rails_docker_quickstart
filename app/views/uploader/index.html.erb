<h1>Uploader#index</h1>


<%= link_to 'uploade', action: "form" %>
    <ul>
      <% UploadFile.all.reverse_order.each do |upload| %>
        <li><%= link_to upload.name, action: "download", id: upload.id %>
        <td><%= link_to 'Destroy', uploader_delete_path(upload),
                    method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>

      <table>
        <tr>
      <% @upload_file.each do |upload| %>
         
         </tr>
       <% end %>
     </table>

    </ul>
