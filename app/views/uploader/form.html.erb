<h1>Uploader#form</h1>


<% @upload = UploadFile.new %>
   <%= form_for @upload, :url => {:action => :upload} do |f| %>

     <%= f.label :name, "name" %>
     <%= f.text_field :name %><br>
     <br>
     <%= f.label :file %>
     <%= f.file_field :file %><br>
<br>
     <%= f.submit "Upload" %>
   <% end %>
